{
  "name": "antelopejs-nuxt2-bootstrap-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt",
    "build-a": "cross-env NODE_ENV=development nuxt build -a",
    "build-dev": "cross-env NODE_ENV=development nuxt build",
    "start-dev": "cross-env NODE_ENV=development nuxt start",
    "build-staging": "cross-env NODE_ENV=staging nuxt build",
    "start-staging": "cross-env NODE_ENV=staging nuxt start",
    "build-prod": "cross-env NODE_ENV=production nuxt build",
    "start-prod": "cross-env NODE_ENV=production nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:js:fix": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint:style:fix": "stylelint **/*.{vue,css} --ignore-path .gitignore --fix",
    "lint": "yarn lint:js && yarn lint:style",
    "lint:fix": "yarn lint:js:fix && yarn lint:style:fix",
    "test": "jest",
    "postinstall": "patch-package && husky install"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.4",
    "aos": "^2.3.4",
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.12.1",
    "cross-env": "^7.0.3",
    "nuxt": "^2.15.6",
    "nuxt-gmaps": "^1.2.10",
    "nuxt-property-decorator": "^2.9.1",
    "particles.vue": "^2.13.4",
    "patch-package": "^6.4.7",
    "spdy": "^4.0.2",
    "tsparticles": "^1.30.4",
    "vue-i18n": "^8.24.4",
    "vue-sweetalert2": "^4.3.1",
    "vuelidate": "^0.7.6",
    "vuelidate-property-decorators": "^1.0.28"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@nuxt/types": "^2.15.6",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/color-mode": "^2.0.9",
    "@nuxtjs/eslint-config-typescript": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@prettier/plugin-pug": "^1.15.2",
    "@types/vue-i18n": "^7.0.0",
    "@types/vuelidate": "^0.7.14",
    "@vue/test-utils": "^1.2.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.9.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "node-sass": "^5.0.0",
    "prettier": "^2.3.0",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "ts-jest": "^26.5.6",
    "vue-jest": "^3.0.7"
  }
}
